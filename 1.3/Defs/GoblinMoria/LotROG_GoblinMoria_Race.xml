        </li>
      </alienwandererkinds>
    </pawnKindSettings>
  </AlienRace.RaceSettings>
  <AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>Alien_GoblinMoria</defName>
    <label>moria goblin</label>
    <description>Moria Goblin description</description>
    <alienRace>
      <generalSettings>
        <maleGenderProbability>0.9</maleGenderProbability>
        <allowHumanBios>false</allowHumanBios>
        <forcedRaceTraitEntries>
          <li>
            <defName>Bloodlust</defName>
            <chance>50</chance>
          </li>
          <li>
            <defName>Psychopath</defName>
            <chance>75</chance>
          </li>
          <li>
            <defName>Abrasive</defName>
            <chance>50</chance>
          </li>
          <li>
            <defName>AnnoyingVoice</defName>
            <chance>50</chance>
          </li>
          <li>
            <defName>Neurotic</defName>
            <chance>50</chance>
            <degree>1</degree>
          </li>
          <li>
            <defName>Nerves</defName>
            <chance>25</chance>
            <degree>-1</degree>
          </li>
        </forcedRaceTraitEntries>
        <!--<maxDamageForSocialfight>6</maxDamageForSocialfight>-->
        <alienPartGenerator>
          <aliencrowntypes>
            <li>Average_Normal</li>
          </aliencrowntypes>
          <alienbodytypes>
            <li>Thin</li>
          </alienbodytypes>
          <useGenderedHeads>false</useGenderedHeads>
          <bodyAddons>
            <li>
              <path>MoriaGoblin/Ears/moriaEar</path>
              <bodyPart>Head</bodyPart>
              <inFrontOfBody>true</inFrontOfBody>
              <defaultOffset>Tail</defaultOffset>
              <offsets>
                <south>
                  <layerOffset>0</layerOffset>
                  <bodyTypes>
                    <Male>(-0.42,0.36)</Male>
                    <Female>(-0.42,0.36)</Female>
                    <Fat>(-0.42,0.36)</Fat>
                    <Thin>(-0.42,0.36)</Thin>
                  </bodyTypes>
                  <crownTypes>
                    <Average_Normal>(0,0.00)</Average_Normal>
                  </crownTypes>
                </south>
                <north>
                  <layerOffset>0</layerOffset>
                  <bodyTypes>
                    <Male>(0,0.69)</Male>
                    <Female>(0,0.69)</Female>
                    <Fat>(0,0.69)</Fat>
                    <Thin>(0,0.69)</Thin>
                  </bodyTypes>
                  <crownTypes>
                    <Average_Normal>(0,0.0)</Average_Normal>
                  </crownTypes>
                </north>
                <east>
                  <layerOffset>0</layerOffset>
                  <bodyTypes>
                    <Male>(-0.5,0.35)</Male>
                    <Female>(-0.5,0.35)</Female>
                    <Fat>(-0.5,0.35)</Fat>
                    <Thin>(-0.5,0.35)</Thin>
                  </bodyTypes>
                  <crownTypes>
                    <Average_Normal>(0,0.0)</Average_Normal>
                  </crownTypes>
                </east>
              </offsets>
              <colorChannel>skin</colorChannel>
              <hiddenUnderApparelFor>
                <li>FullHead</li>
              </hiddenUnderApparelFor>
            </li>
          </bodyAddons>
          <colorChannels>
            <li>
              <name>skin</name>
              <first Class="ColorGenerator_Options">
                <options>
                  <li>
                    <weight>1</weight>
                    <only>RGBA(1.0,1.0,1.0,1)</only>
                  </li>
                  <li>
                    <weight>5</weight>
                    <only>RGBA(0.5,0.5,0.5,1)</only>
                  </li>
                  <li>
                    <weight>5</weight>
                    <only>RGBA(0.3,0.3,0.3,1)</only>
                  </li>
                  <li>
                    <weight>15</weight>
                    <only>RGBA(0.9,0.9,0.7,1)</only>
                  </li>
                  <li>
                    <weight>15</weight>
                    <only>RGBA(0.6,0.6,0.4,1)</only>
                  </li>
                  <li>
                    <weight>15</weight>
                    <only>RGBA(0.8,0.8,0.6,1)</only>
                  </li>
                  <li>
                    <weight>5</weight>
                    <only>RGBA(0.3,0.3,0.1,1)</only>
                  </li>
                  <li>
                    <weight>2</weight>
                    <only>RGBA(0.3,0.5,0.1,1)</only>
                  </li>
                  <li>
                    <weight>2</weight>
                    <only>RGBA(0.5,0.7,0.3,1)</only>
                  </li>
                  <li>
                    <weight>2</weight>
                    <only>RGBA(0.7,0.9,0.5,1)</only>
                  </li>
                  <li>
                    <weight>1</weight>
                    <only>RGBA(0.3,0.5,0.6,1)</only>
                  </li>
                </options>
              </first>
            </li>
          </colorChannels>
          <customDrawSize>(0.84, 0.84)</customDrawSize>
          <customHeadDrawSize>(0.84, 0.84)</customHeadDrawSize>
          <customPortraitDrawSize>(0.84, 0.84)</customPortraitDrawSize>
          <headOffset>(0, -0.15)</headOffset>
        </alienPartGenerator>
      </generalSettings>
      <graphicPaths>
        <li>
          <head>MoriaGoblin/Heads/</head>
        </li>
      </graphicPaths>
      <styleSettings>
        <li>
          <key>HairDef</key>
          <value>
            <styleTagsOverride>
              <li>LotROG_OrcGoblin</li>
            </styleTagsOverride>
          </value>
        </li>
        <li>
          <key>BeardDef</key>
          <value>
            <styleTagsOverride>
              <li>NoBeard</li>
            </styleTagsOverride>
          </value>
        </li>
      </styleSettings>
      <thoughtSettings>
        <cannotReceiveThoughts>
          <li>EnvironmentDark</li>
          <li>NeedOutdoors</li>
          <li>NeedBeauty</li>
        </cannotReceiveThoughts>
        <replacerList>
          <li>
            <original>AteInImpressiveDiningRoom</original>
            <replacer>LotROG_AteInImpressiveDiningRoom</replacer>
          </li>
          <li>
            <original>JoyActivityInImpressiveRecRoom</original>
            <replacer>LotROG_JoyActivityInImpressiveRecRoom</replacer>
          </li>
          <li>
            <original>SleptInBedroom</original>
            <replacer>LotROG_SleptInBedroom</replacer>
          </li>
          <li>
            <original>SleptInBarracks</original>
            <replacer>LotROG_SleptInBarracks</replacer>
          </li>
          <!-- <li> -->
          <!-- <original>NeedBeauty</original> -->
          <!-- <replacer>LotROG_NeedBeauty</replacer> -->
          <!-- </li> -->
        </replacerList>
      </thoughtSettings>
    </alienRace>
    <!--<uiIconPath>Things/Pawn/Humanlike/UI/IconHuman</uiIconPath>-->
    <statBases>
      <MarketValue>500</MarketValue>
      <MoveSpeed>4.5</MoveSpeed>
      <Flammability>1.0</Flammability>
      <ComfyTemperatureMin>-60</ComfyTemperatureMin>
      <ComfyTemperatureMax>60</ComfyTemperatureMax>
      <LeatherAmount>20</LeatherAmount>
      <ResearchSpeed>1.0</ResearchSpeed>
    </statBases>
    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>1.6</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>5</power>
        <cooldownTime>1.6</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>3</power>
        <cooldownTime>1.8</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <lifeExpectancy>80</lifeExpectancy>
      <leatherDef>LotROG_Leather_Goblin</leatherDef>
      <nameCategory>HumanStandard</nameCategory>
      <body>Human</body>
      <baseBodySize>0.6</baseBodySize>
      <baseHealthScale>0.6</baseHealthScale>
      <foodType>CarnivoreAnimal, AnimalProduct, Meat, Kibble, Processed, Corpse</foodType>
      <gestationPeriodDays>20</gestationPeriodDays>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, .25)</li>
          <li>(2, .25)</li>
          <li>(3, .25)</li>
          <li>(4, .25)</li>
          <li>(5, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeStageAges>
        <li>
          <def>HumanlikeBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>HumanlikeToddler</def>
          <minAge>1</minAge>
        </li>
        <li>
          <def>HumanlikeChild</def>
          <minAge>2</minAge>
        </li>
        <li>
          <def>HumanlikeTeenager</def>
          <minAge>3</minAge>
        </li>
        <li>
          <def>HumanlikeAdult</def>
          <minAge>5</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(3,0)</li>
          <li>(5,100)</li>
          <li>(10,50)</li>
          <li>(15,5)</li>
          <li>(20,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>OrganicStandard</li>
      </hediffGiverSets>
    </race>
    <recipes>
      <li>InstallPowerClaw</li>
      <li>InstallBionicEye</li>
      <li>InstallBionicArm</li>
      <li>InstallBionicLeg</li>
      <li>InstallSimpleProstheticArm</li>
      <li>InstallSimpleProstheticLeg</li>
      <li>InstallPegLeg</li>
      <li>InstallDenture</li>
      <li>InstallJoywire</li>
      <li>InstallPainstopper</li>
      <li>InstallNaturalHeart</li>
      <li>InstallNaturalLung</li>
      <li>InstallNaturalKidney</li>
      <li>InstallNaturalLiver</li>
      <li>ExciseCarcinoma</li>
      <li>AdministerMechSerumHealer</li>
      <li>RemoveBodyPart</li>
      <li>Euthanize</li>
      <li>Anesthetize</li>
    </recipes>
  </AlienRace.ThingDef_AlienRace>
</Defs>