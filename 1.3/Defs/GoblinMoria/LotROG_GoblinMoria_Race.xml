<Defs>
  <AlienRace.RaceSettings>
    <defName>LotROG_GoblinMoriaSettings</defName>
    <pawnKindSettings>
      <startingColonists>
        <li>
          <pawnKindEntries>
            <li>
              <kindDefs>
                <li>LotROG_GoblinMoriaColonist</li>
              </kindDefs>
              <chance>10.0</chance>
            </li>
          </pawnKindEntries>
          <factionDefs>
            <li>PlayerColony</li>
          </factionDefs>
        </li>
      </startingColonists>
      <alienslavekinds>
        <li>
          <kindDefs>
            <li>LotROG_GoblinMoria</li>
          </kindDefs>
          <chance>5.0</chance>
        </li>
      </alienslavekinds>
      <alienrefugeekinds>
        <li>
          <kindDefs>
            <li>LotROG_GoblinMoria</li>
          </kindDefs>
          <chance>10.0</chance>
        </li>
      </alienrefugeekinds>
      <alienwandererkinds>
        <li>
          <pawnKindEntries>
            <li>
              <kindDefs>
                <li>LotROG_GoblinMoria</li>
              </kindDefs>
              <chance>10.0</chance>
            </li>
          </pawnKindEntries>
          <factionDefs>
            <li>PlayerColony</li>
