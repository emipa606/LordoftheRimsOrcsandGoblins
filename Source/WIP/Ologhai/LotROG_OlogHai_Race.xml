<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <AlienRace.RaceSettings>
    <defName>LotROG_OlogHaiSettings</defName>
    <pawnKindSettings>
      <startingColonists>
        <li>
          <pawnKindEntries>
            <li>
              <kindDefs>
                <li>LotROG_OlogHaiColonist</li>
              </kindDefs>
              <chance>10.0</chance>
            </li>
          </pawnKindEntries>
          <factionDefs>
            <li>PlayerColony</li>
          </factionDefs>
        </li>
      </startingColonists>
      <alienslavekinds>
        <li>
          <kindDefs>
            <li>OlogHai</li>
          </kindDefs>
          <chance>5.0</chance>
        </li>
      </alienslavekinds>
      <alienrefugeekinds>
        <li>
          <kindDefs>
            <li>OlogHai</li>
          </kindDefs>
          <chance>10.0</chance>
        </li>
      </alienrefugeekinds>
      <alienwandererkinds>
        <li>
          <pawnKindEntries>
            <li>
              <kindDefs>
                <li>OlogHai</li>
              </kindDefs>
              <chance>10.0</chance>
            </li>
          </pawnKindEntries>
          <factionDefs>
            <li>PlayerColony</li>
          </factionDefs>
        </li>
      </alienwandererkinds>
    </pawnKindSettings>
  </AlienRace.RaceSettings>
  <AlienRace.ThingDef_AlienRace ParentName="BasePawn">
    <defName>Alien_OlogHai</defName>
    <label>olog hai</label>
    <description>OlogHai description</description>
    <alienRace>
      <generalSettings>
        <maleGenderProbability>0.9</maleGenderProbability>
        <allowHumanBios>false</allowHumanBios>
        <forcedRaceTraitEntries>
          <li>
            <defName>Bloodlust</defName>
            <chance>75</chance>
          </li>
          <li>
            <defName>Psychopath</defName>
            <chance>75</chance>
          </li>
          <li>
            <defName>Masochist</defName>
            <chance>75</chance>
          </li>
          <li>
            <defName>CreepyBreathing</defName>
            <chance>50</chance>
          </li>
          <li>
            <defName>Nerves</defName>
            <chance>25</chance>
            <degree>2</degree>
          </li>
          <li>
            <defName>Nerves</defName>
            <chance>50</chance>
            <degree>1</degree>
          </li>
        </forcedRaceTraitEntries>
        <!--<maxDamageForSocialfight>6</maxDamageForSocialfight>-->
        <alienPartGenerator>
          <aliencrowntypes>
            <li>Average_Normal</li>
          </aliencrowntypes>
          <alienbodytypes>
            <li>Hulk</li>
          </alienbodytypes>
          <useGenderedHeads>false</useGenderedHeads>
          <colorChannels>
            <li>
              <name>skin</name>
              <first Class="ColorGenerator_Options">
                <options>
                  <li>
                    <weight>5</weight>
                    <only>RGBA(1.0,1.0,1.0,1)</only>
                  </li>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.5,0.5,0.5,1)</only>
                  </li>
                  <li>
                    <weight>10</weight>
                    <only>RGBA(0.3,0.3,0.3,1)</only>
                  </li>
                  <li>
                    <weight>15</weight>
                    <only>RGBA(0.8,0.5,0.5,1)</only>
                  </li>
                  <li>
                    <weight>15</weight>
                    <only>RGBA(0.8,0.3,0.3,1)</only>
                  </li>
                </options>
              </first>
            </li>
          </colorChannels>
          <customDrawSize>(1.725, 1.725)</customDrawSize>
          <customHeadDrawSize>(0.7, 0.7)</customHeadDrawSize>
          <customPortraitDrawSize>(1.0, 1.0)</customPortraitDrawSize>
          <headOffset>(0.0, 0.2)</headOffset>
        </alienPartGenerator>
      </generalSettings>
      <graphicPaths>
        <li>
          <head>OlogHai/Heads/</head>
        </li>
      </graphicPaths>
      <styleSettings>
        <li>
          <key>HairDef</key>
          <value>
            <styleTagsOverride>
              <li>LotROG_OrcGoblin</li>
            </styleTagsOverride>
          </value>
        </li>
        <li>
          <key>BeardDef</key>
          <value>
            <styleTagsOverride>
              <li>NoBeard</li>
            </styleTagsOverride>
          </value>
        </li>
      </styleSettings>
      <thoughtSettings>
        <cannotReceiveThoughts>
          <li>EnvironmentDark</li>
          <li>NeedOutdoors</li>
        </cannotReceiveThoughts>
        <replacerList>
          <li>
            <original>AteInImpressiveDiningRoom</original>
            <replacer>LotROG_AteInImpressiveDiningRoom</replacer>
          </li>
          <li>
            <original>JoyActivityInImpressiveRecRoom</original>
            <replacer>LotROG_JoyActivityInImpressiveRecRoom</replacer>
          </li>
          <li>
            <original>SleptInBedroom</original>
            <replacer>LotROG_SleptInBedroom</replacer>
          </li>
          <li>
            <original>SleptInBarracks</original>
            <replacer>LotROG_SleptInBarracks</replacer>
          </li>
          <li>
            <original>NeedBeauty</original>
            <replacer>LotROG_NeedBeauty</replacer>
          </li>
        </replacerList>
      </thoughtSettings>
    </alienRace>
    <!--<uiIconPath>Things/Pawn/Humanlike/UI/IconHuman</uiIconPath>-->
    <statBases>
      <MarketValue>1500</MarketValue>
      <MoveSpeed>5</MoveSpeed>
      <Flammability>1.0</Flammability>
      <ComfyTemperatureMin>-60</ComfyTemperatureMin>
      <ComfyTemperatureMax>60</ComfyTemperatureMax>
      <LeatherAmount>25</LeatherAmount>
      <ResearchSpeed>0.10</ResearchSpeed>
    </statBases>
    <tools>
      <li>
        <label>left fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.6</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>right fist</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.6</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
        <surpriseAttack>
          <extraMeleeDamages>
            <li>
              <def>Stun</def>
              <amount>22</amount>
            </li>
          </extraMeleeDamages>
        </surpriseAttack>
      </li>
      <li>
        <label>head</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>6</power>
        <cooldownTime>1.8</cooldownTime>
        <linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
        <chanceFactor>0.2</chanceFactor>
      </li>
    </tools>
    <race>
      <thinkTreeMain>Humanlike</thinkTreeMain>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <lifeExpectancy>80</lifeExpectancy>
      <leatherDef>LotROG_Leather_Goblin</leatherDef>
      <nameCategory>HumanStandard</nameCategory>
      <body>Human</body>
      <baseBodySize>1</baseBodySize>
      <baseHealthScale>1</baseHealthScale>
      <foodType>CarnivoreAnimal, AnimalProduct, Meat, Kibble, Processed, Corpse</foodType>
      <gestationPeriodDays>25</gestationPeriodDays>
      <litterSizeCurve>
        <points>
          <li>(0.5, 0)</li>
          <li>(1, .25)</li>
          <li>(2, .25)</li>
          <li>(3, .25)</li>
          <li>(4, .25)</li>
          <li>(5, 0)</li>
        </points>
      </litterSizeCurve>
      <lifeStageAges>
        <li>
          <def>HumanlikeBaby</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>HumanlikeToddler</def>
          <minAge>1</minAge>
        </li>
        <li>
          <def>HumanlikeChild</def>
          <minAge>2</minAge>
        </li>
        <li>
          <def>HumanlikeTeenager</def>
          <minAge>3</minAge>
        </li>
        <li>
          <def>HumanlikeAdult</def>
          <minAge>5</minAge>
        </li>
      </lifeStageAges>
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <specialShadowData>
        <volume>(0.3, 0.8, 0.4)</volume>
        <offset>(0,0,-0.3)</offset>
      </specialShadowData>
      <ageGenerationCurve>
        <points>
          <li>(3,0)</li>
          <li>(5,100)</li>
          <li>(10,100)</li>
          <li>(15,50)</li>
          <li>(20,25)</li>
          <li>(25,10)</li>
          <li>(30,0)</li>
        </points>
      </ageGenerationCurve>
      <hediffGiverSets>
        <li>OrganicStandard</li>
      </hediffGiverSets>
    </race>
    <recipes>
      <li>InstallPowerClaw</li>
      <li>InstallBionicEye</li>
      <li>InstallBionicArm</li>
      <li>InstallBionicLeg</li>
      <li>InstallSimpleProstheticArm</li>
      <li>InstallSimpleProstheticLeg</li>
      <li>InstallPegLeg</li>
      <li>InstallDenture</li>
      <li>InstallJoywire</li>
      <li>InstallPainstopper</li>
      <li>InstallNaturalHeart</li>
      <li>InstallNaturalLung</li>
      <li>InstallNaturalKidney</li>
      <li>InstallNaturalLiver</li>
      <li>ExciseCarcinoma</li>
      <li>AdministerMechSerumHealer</li>
      <li>RemoveBodyPart</li>
      <li>Euthanize</li>
      <li>Anesthetize</li>
    </recipes>
  </AlienRace.ThingDef_AlienRace>
</Defs>